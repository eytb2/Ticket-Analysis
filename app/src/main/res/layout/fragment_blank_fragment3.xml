<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:app="http://schemas.android.com/apk/res-auto"
              xmlns:tools="http://schemas.android.com/tools"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical"
              tools:context=".ui.BlankFragment2">

    <cn.lemon.ticketsystem.ui.TitleBarUtil
        android:layout_width="match_parent"
        android:id="@+id/mTitleBarUtil"
        android:background="#e20000"

        android:layout_height="50dp"></cn.lemon.ticketsystem.ui.TitleBarUtil>



    <android.support.v4.widget.SwipeRefreshLayout
        android:id="@+id/srf"
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <WebView
            android:id="@+id/webview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/d50"></WebView>

    </android.support.v4.widget.SwipeRefreshLayout>

</LinearLayout>